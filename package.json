{
  "name": "gulp-browser",
  "version": "3.0.2",
  "description": "browserify and other goodies for gulp",
  "main": "dist/index.js",
  "scripts": {
    "test": "(npmts)",
    "reinstall": "(rm -r node_modules && npm install)",
